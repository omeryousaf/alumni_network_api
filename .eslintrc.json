{
    "env": {
        "es6": true,
        "mocha": true,
        "jest": true,
        "node": true
    },
    "extends": ["eslint:recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "esprima",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [],
    "settings": {},
    "rules": {
        "no-console": "off"
    }
}